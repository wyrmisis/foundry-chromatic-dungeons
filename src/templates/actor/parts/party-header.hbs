<header class="sheet-header sheet-header--party">
  <img
    class="profile-img"
    src="{{actor.img}}"
    data-edit="img"
    title="{{actor.name}}"/>
  <div class="sheet-header__container">
    <input
      class="name"
      name="name"
      type="text"
      value="{{actor.name}}"
      placeholder="Name"/>
</header>