<div class="biography">
  {{!-- If you want TinyMCE editors to output inline rolls when rendered, you need to pass the actor's roll data to the rollData property. --}}
      
  <div class="biography__appearance">
    <img
      class="biography__portrait"
      src="{{actor.img}}"
      data-edit="img"
      title="{{actor.name}}"
      height="100"
      width="100"/>

    <h2>Description</h2>
    {{editor
      content=data.description
      target="data.description"
      rollData=rollData
      button=true
      owner=owner
      editable=editable
    }}
  </div>

  <h2>Biography</h2>
  {{editor
    content=data.bio
    target="data.bio"
    rollData=rollData
    button=true
    owner=owner
    editable=editable
  }}
</div>