{{#each data.attributes as |attribute key|}}
  <div class="attributes__item">
    <input
      type="number"
      min="0"
      name="system.attributes.{{key}}"
      value="{{attribute}}"
      data-dtype="Number"/>
    <label
      class="rollable rollable--no-icon"
      for="attributes.{{key}}"
      data-roll="1d20-{{lookup ../data.attributeMods key}}"
      data-target="{{attribute}}"
      data-roll-type="Attribute"
      data-label="{{localizeAttr key}}">
      {{localizeAttr key}}
    </label>
  </div>
{{/each}}